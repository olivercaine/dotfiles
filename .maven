#!/usr/bin/env bash

alias mi='mvn install'                              # (m)aven (i)nstall
alias miu='mvn install -U'                          # (m)aven (i)nstall with (u)pdate
alias mist='mvn install -DskipTests'                # (m)aven (i)nstall (s)kip (t)ests
alias mci='mvn clean install'                       # (m)aven (c)lean (i)nstall
alias mciu='mvn clean install -U'                   # (m)aven (c)lean (i)nstall with (u)pdate
alias mcist='mvn clean -DskipTests install'         # (m)aven (c)lean (i)nstall (s)kip (t)ests
alias mcistu='mvn clean -DskipTests install -U'     # (m)aven (c)lean (i)nstall (s)kip (t)ests with (u)pdate